{
    "name": "tonysong/dashq",
    "description": "A Dashboard for Laravel Queue with database driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Tony Song",
            "email": "tonywei92@gmail.com",
            "homepage": "https://tonywei92.github.io"
        }
    ],
    "homepage": "https://github.com/tonysong/dashq",
    "keywords": ["Laravel", "DashQ"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "TonySong\\DashQ\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TonySong\\DashQ\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TonySong\\DashQ\\DashQServiceProvider"
            ],
            "aliases": {
                "DashQ": "TonySong\\DashQ\\Facades\\DashQ"
            }
        }
    }
}
